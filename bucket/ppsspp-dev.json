        "regex": "rev=v(?<version>[\\d.]+)-(?<build>[\\d]+)-g(?<commit>[\\da-fA-F]+)&(?:amp;)platform=windows-amd64",
        "replace": "${version}-${build}-g${commit}"
    },
    "autoupdate": {
